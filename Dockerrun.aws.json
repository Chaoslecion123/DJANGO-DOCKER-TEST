{
    "AWSEBDockerrunVersion": 3,
    "containerDefinitions": [
        {
            "name": "django",
            "image": "862363392700.dkr.ecr.us-east-1.amazonaws.com/django-app",
            "hostname": "django",
            "essential": true,
            "memory": 1024,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000
                }
            ]
        },
        {
            "name": "caddy",
            "image": "862363392700.dkr.ecr.us-east-1.amazonaws.com/caddy-app",
            "hostname": "caddy",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                },
                {
                    "hostPort": 443,
                    "containerPort": 443
                }
            ],
            "links": ["django"],
            "memory": 512
        }
    ]
}